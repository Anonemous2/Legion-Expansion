{
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "browser": true,
    "jquery": true,
    "amd": true
  },
  "settings": {
    "lodash": {
      "version": 3,
      "pragma": "_"
    }
  },
  "extends": ["eslint:recommended"],
  "globals": {
    "api": "readonly",
    "model": "readonly",
    "_": "readonly",
    "loc": "readonly",
    "locUpdateDocument": "readonly",
    "ko": "readonly",
    "engine": "readonly",
    "loadCSS": "readonly",
    "loadHtml": "readonly",
    "loadScript": "readonly",
    "gBuild": "readonly",
    "Build": "readonly",
    "legionglobal": "readonly",
    "handlers": "readonly"
  },
  "plugins": ["lodash"],
  "rules": {
    "no-inner-declarations": "off",
    "lodash/callback-binding": 2,
    "lodash/collection-method-value": 2,
    "lodash/collection-return": 2,
    "lodash/no-commit": 2,
    "lodash/no-double-unwrap": 2,
    "lodash/no-extra-args": 2,
    "lodash/unwrap": 2
  }
}
